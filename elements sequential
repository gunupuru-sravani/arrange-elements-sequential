#include <stdio.h>
 int main()    
{    
    int a[100],n;
    int temp = 0;    
     printf("enter the size of array");
scanf("%d",&n);
printf("enter the elements\n");
for(int i=0;i<n;i++)
scanf("%d",&a[i]);
    for (int i=0;i<n-1;i++)
    {     
        for (int j=i+1;j<n;j++) 
        {     
           if(a[i]>a[j]) 
           {    
               temp = a[i];    
               a[i] = a[j];    
               a[j] = temp;    
           }     
        }     
    }    
    printf("Elements in sequential order\n");    
    for (int i=0;i<n;i++) 
    {     
        printf("%d ",a[i]);    
    }    
    return 0;    
}     
